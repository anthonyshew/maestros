{
  "name": "maestros",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier . --check --cache --cache-location='node_modules/.cache/prettiercache'",
    "format:fix": "prettier . --write --cache --cache-location='node_modules/.cache/prettiercache' --loglevel=warn",
    "generate": "contentlayer build",
    "lint": "eslint . --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
    "lint:fix": "eslint . --fix --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
    "spellcheck": "spellchecker --files '**/*.mdx' --dictionaries 'dictionary.txt' --no-suggestions --quiet",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.1",
    "@repo/analytics": "workspace:*",
    "@repo/db": "workspace:*",
    "@repo/ui": "workspace:*",
    "@vercel/og": "^0.0.27",
    "bright": "^0.8.2",
    "class-variance-authority": "^0.6.1",
    "contentlayer": "latest",
    "date-fns": "^2.30.0",
    "framer-motion": "^8.5.5",
    "lucide-react": "^0.108.0",
    "next": "^13.4.19",
    "next-auth": "^4.22.1",
    "next-contentlayer": "latest",
    "next-themes": "^0.2.1",
    "react": "latest",
    "react-dom": "latest",
    "react-wrap-balancer": "^0.4.1",
    "reactflow": "^11.8.3",
    "rehype-mdx-code-props": "^1.0.0",
    "spellchecker-cli": "^6.1.1",
    "tailwind-merge": "^1.13.2",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@repo/lint": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@tailwindcss/typography": "^0.5.9",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "autoprefixer": "^10.4.14",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "postcss": "^8.4.24",
    "prisma": "^5.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "latest"
  },
  "engines": {
    "node": "18"
  }
}
